public class Fenetre2 {
	
	private JTextField texte1 = new JTextField();

	public Fenetre2() {
		setVisible(true);
		setSize(200, 200);
		setLocationRelativeTo(null);
		setTitle("Fenetre2");
		setLayout(null);
