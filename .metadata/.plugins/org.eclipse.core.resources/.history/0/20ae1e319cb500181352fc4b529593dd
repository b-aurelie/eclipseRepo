package exercices;

public class challenge {

	public static void main(String[] args) {
		
		int tab[] = {0,0,0,0};
		int i = tab.length-1;
		while (i>1) {
			if (tab[i]==9) {
				tab[i]=0;
				i--;}
				while (tab[i]<=9) {
				tab[i]++;
				System.out.println(""+tab[0]+"."+tab[1]+"."+tab[2]+"."+tab[3]);
				
				
			}
		}
	}
}
	

