package exercices;

public class challenge {

	public static void main(String[] args) {
		
		int tab[] = {0,0,0,0};
		while (tab[0]<10 && tab[1]<10 && tab[2]<10) {
			while (tab[0]<10 && tab[1]<10 && tab[2]<10 && tab[3]<10) {
				System.out.println(""+tab[0]+tab[1]+tab[2]+tab[3]);
				tab[3]++;
			}
			tab[2]++;
		}
	}
}