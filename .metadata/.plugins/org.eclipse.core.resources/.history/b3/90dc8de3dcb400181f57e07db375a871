package exercices;

public class exercices {

	public static void main(String[] args) {
		
		//TRI A BULLES
		
		int tab[] = {8,7,0,5,9,2,3,1,4,6};
		int temp = 0;

		for(int i=tab.length-1;i>0;i--) {
			for(int j=0;j<i;j++) {
				if(tab[j+1]<tab[j]) {
					temp = tab[j+1];
					tab[j+1] = tab[j];
					tab[j] = temp;
				}
			}
		}
		for (int i=0;i<tab.length;i++) {
			System.out.print(tab[i]);
		}
	}	
}
