package exercices;

public class challenge {

	public static void main(String[] args) {
		
		int tab[] = {0,0,0,0};
		int i = tab.length-1;
		while (tab[i]<=9) {
			tab[i]++;
			if (tab[i]==9) {
				i--;
				tab[i]=0;
			}
			System.out.println(""+tab[0]+"."+tab[1]+"."+tab[2]+"."+tab[3]);
		}
	}
}
	

